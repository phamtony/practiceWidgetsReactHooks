Redux/widgets/src/components/Accordion.js",[],"/Users/tonypham/Sites/Courses/Modern React with Redux/widgets/src/components/Search.js",["37"],"import React, {useState, useEffect} from \"react\";\nimport axios from \"axios\";\n\nconst Search = () => {\n    const [term, setTerm] = useState('programming');\n    const [results, setResults] = useState([]);\n\n    useEffect(() => {\n        const search = async () => {\n            const {data} = await axios.get('https://en.wikipedia.org/w/api.php', {\n               params: {\n                   action: 'query',\n                   list: 'search',\n                   origin: '*',\n                   format: 'json',\n                   srsearch: term\n               }\n            });\n\n            setResults(data.query.search);\n        }\n\n        if (term && !results.length) {\n            search();\n        } else {\n            const timeOutID = setTimeout(() => {\n                if (term) {\n                    search();\n                }\n            }, 500);\n\n            return () => {\n                clearTimeout(timeOutID);\n            }\n        }\n\n    }, [term]);\n\n    const renderedResults = results.map((result) => {\n        return (\n            <div key={result.pageid} className=\"item\">\n                <div className=\"right floated content\">\n                    <a\n                        href={`https://en.wikipedia.org?curid=${result.pageid}`}\n                        className=\"ui button\">\n                        Go\n                    </a>\n                </div>\n                <div className=\"content\">\n                    <div className=\"header\">\n                        {result.title}\n                    </div>\n                    <span dangerouslySetInnerHTML={{ __html: result.snippet}}></span>\n                </div>\n            </div>\n        );\n    });\n\n    return (\n        <div>\n            <div className=\"ui form\">\n                <div className=\"field\">\n                    <label htmlFor=\"\">Enter Search Term</label>\n                    <input\n                        type=\"text\"\n                        className=\"input\"\n                        value={(term)}\n                        onChange={e => setTerm(e.target.value)}\n                    />\n                </div>\n            </div>\n            <div className=\"ui celled list\">\n                {renderedResults}\n            </div>\n        </div>\n    );\n}\n\nexport default Search;","/Users/tonypham/Sites/Courses/Modern React with Redux/widgets/src/components/Dropdown.js",[],"/Users/tonypham/Sites/Courses/Modern React with Redux/widgets/src/components/Translate.js",[],{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","replacedBy":"41"},{"ruleId":"42","severity":1,"message":"43","line":1,"column":16,"nodeType":"44","messageId":"45","endLine":1,"endColumn":24},{"ruleId":"42","severity":1,"message":"46","line":2,"column":8,"nodeType":"44","messageId":"45","endLine":2,"endColumn":17},{"ruleId":"42","severity":1,"message":"47","line":3,"column":8,"nodeType":"44","messageId":"45","endLine":3,"endColumn":14},{"ruleId":"42","severity":1,"message":"48","line":4,"column":8,"nodeType":"44","messageId":"45","endLine":4,"endColumn":16},{"ruleId":"42","severity":1,"message":"49","line":8,"column":7,"nodeType":"44","messageId":"45","endLine":8,"endColumn":12},{"ruleId":"42","severity":1,"message":"50","line":23,"column":7,"nodeType":"44","messageId":"45","endLine":23,"endColumn":14},{"ruleId":"51","severity":1,"message":"52","line":38,"column":1,"nodeType":"53","endLine":45,"endColumn":2},{"ruleId":"54","severity":1,"message":"55","line":37,"column":8,"nodeType":"56","endLine":37,"endColumn":14,"suggestions":"57"},"no-native-reassign",["58"],"no-negated-in-lhs",["59"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'Accordion' is defined but never used.","'Search' is defined but never used.","'Dropdown' is defined but never u